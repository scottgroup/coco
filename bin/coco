#!/bin/bash
DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
pyv="$(python3 -V 2>&1 | cut -d ' ' -f 2)"
if [ "${pyv:0:1}" == "3" ];
then
    if [ "$pyv" '<' 3.5 ]; 
        then echo 'Exiting. Please use python 3.5 or greater'; exit 1;
        else python3 $DIR/coco.py $@;
    fi
else
     echo 'Exiting. Please use python 3.5 or greater'; exit 1;
fi
